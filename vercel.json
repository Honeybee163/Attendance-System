{
    "builds": [
        {
            "src": "ATTENDANCE/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "20mb","runtime": "python3.10"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "ATTENDANCE/wsgi.py"
        }
    ]
}